@model List<model.EF.Product>
   

    @foreach (var iteam in Model)
    {
        var url = "/chi-tiet/" +iteam.MetaTitle+"-" + iteam.ID;
    <div class="grid_1_of_4 images_1_of_4">
        <a href="@url"><img src="/Content/images/@iteam.Image" alt="" /></a>
        <h2>@iteam.Name </h2>
    
        <div class="price-details">
            <div class="price-number">
                <p><span class="rupees">@iteam.Price.ToString("N0")</span></p>
            </div>
            <div class="add-cart">
                <h4><a href="/them-gio-hang?productId=@iteam.ID&quantity=1">Mua ngay</a></h4>
            </div>
            <div class="clear"></div>
        </div>

    </div>
    }

       
  
