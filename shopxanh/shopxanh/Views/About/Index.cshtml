@model List<model.EF.About>
@{
    ViewBag.Title = "Giới thiệu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="main">
    <div class="content">
        <div class="section group">
           @foreach (var item in Model)
           {
            <div class="col-md-12">
                <h3>@item.Name</h3>
                <img src="Content/images/@item.Image" alt="">
                <p>@Html.Raw(item.Detail)</p>
                
            </div>
           }

        </div>
    </div>
</div>
